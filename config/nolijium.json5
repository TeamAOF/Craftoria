	// DEFAULT: `false`
	"enableGamma": false,
	// Intended for advanced users and developers only. Colour format is 0xAARRGGBB.
	// DEFAULT: `false`
	"tooltipColourOverride": false,
	"tooltipBorderStart": 0,
	"tooltipBorderEnd": 0,
	"tooltipBackgroundStart": 0,
	"tooltipBackgroundEnd": 0,
	// Intended for advanced users and developers only. Colour format is 0xAARRGGBB.
