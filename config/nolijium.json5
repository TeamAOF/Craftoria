	// DEFAULT: `[ ]`
	"hideParticlesByID": [  ],
	// Higher values cycle faster. Lower values cycle slower.
	// DEFAULT: `0.5`
	"chromaSpeed": 0.5,
	// Enable Chroma on block outlines.
	// DEFAULT: `false`
	"enableChromaBlockOutlines": false,
